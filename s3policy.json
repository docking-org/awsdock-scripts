{
    "Version": "2012-10-17",
    "Id": "s3policy01",
    "Statement": [
        {
            "Sid": "s3statement01",
            "Effect": "Allow",
            "Action": [
		"s3:PutObject",
                "s3:GetObject",
                "s3:GetBucketLocation",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::btingletestbucket/*",
                "arn:aws:s3:::btingletestbucket",
                "arn:aws:s3:::zinc3d",
                "arn:aws:s3:::zinc3d/*"
            ]
        }
    ]
}
